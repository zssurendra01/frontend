pipeline{
    agent any
    stages{
        stage("Installing nginx"){
            steps{
                sh '''
                nginx install
                '''
            }
        }
    }
        stage("Installing npm"){
            steps{
                sh '''
                npm install
                '''
            }
        }
    
    
        stage("seeing artifacts"){
            steps{
                sh '''
                   zip -r frontend.zip *
                '''
            }
        }
 
}