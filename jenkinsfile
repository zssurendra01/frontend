pipeline {
    agent {
        label "SLAVE"
    }
    stages {
        stage('Download ') {
            steps {
                sh '''
                 npm install
                 npm run build
                '''
            }
        }
        stage('seeing artifacts') {
            steps {
                sh '''
                zip -r frontend.zip *
                '''
            }
        }
        stage ('Uploading artifacts to nexus') {
            steps {
                sh '''
                curl -v -u admin:zelar123 --upload-file frontend.zip http://172.31.0.171:8081/repository/frontend/frontend.zip
                '''

            }
        }
    }
} 